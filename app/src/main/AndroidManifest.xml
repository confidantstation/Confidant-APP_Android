        <activity
            android:name=".ui.activity.user.QRCodeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.user.QRFriendCodeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.scan.ScanQrCodeActivity"
