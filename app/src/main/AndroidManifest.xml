        <activity android:name=".ui.activity.main.GuestActivity"
            android:screenOrientation="portrait" />

        <activity android:name=".ui.activity.file.FileChooseActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".ui.activity.selectfriend.selectFriendActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".ui.activity.router.RouterManagementActivity"
            android:screenOrientation="portrait"/>
